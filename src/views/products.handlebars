<h1>Productos</h1>
<ul>
  {{#each products}}
    <li><h2>{{this.title}}</h2>
      <p>Descripcion : {{this.description}}</p>
      <p>Precio: {{this.price}}</p></li>
  {{/each}}
</ul>
{{#if hasNextPage}}
  <a href="/?page={{nextPage}}">Siguiente</a>
{{/if}}
{{#if hasPrevPage}}
  <a href="/?page={{prevPage}}">Anterior</a>
{{/if}}

<p>Pagina{{currentPage}} de {{totalPages}} </p>